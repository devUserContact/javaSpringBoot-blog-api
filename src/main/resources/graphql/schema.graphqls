schema {
  query: Query
}
type Query {
  devGetAllBlogPosts: [DevEntity]
  artGetAllBlogPosts: [ArtEntity]
}
type DevEntity {
  id: ID
  title: String
  sub_title: String
  body: String
  media: String
  tags: String
  submission_date: String
}
type ArtEntity {
  id: ID
  title: String
  subtitle: String
  body: String
  media: String
  tags: String
  submission_date: String
}
